{
  "uid": "spring-boot-dashboard",
  "title": "Generic Metrics Dashboard",
  "overwrite": true,
  "time": {
    "from": "now-5m",
    "to": "now"
  },
  "timezone": "browser",
  "panels": [
    {
      "title": "Total HTTP Requests",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m]))",
          "legendFormat": "Total Requests"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 }
    },
    {
      "title": "HTTP Requests per Endpoint",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum by (endpoint) (rate(http_requests_total[5m]))",
          "legendFormat": "{{endpoint}}"
        }
      ],
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 6 }
    },
    {
      "title": "Average Response Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_sum[5m]) / rate(http_server_requests_seconds_count[5m])",
          "legendFormat": "Response Time (s)"
        }
      ],
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 6 }
    },
    {
      "title": "CPU Usage",
      "type": "gauge",
      "targets": [
        {
          "expr": "process_cpu_usage * 100",
          "legendFormat": "CPU Usage (%)"
        }
      ],
      "gridPos": { "x": 12, "y": 6, "w": 6, "h": 6 }
    },
    {
      "title": "Memory Usage",
      "type": "gauge",
      "targets": [
        {
          "expr": "(process_memory_usage_bytes / process_max_memory_bytes) * 100",
          "legendFormat": "Memory Usage (%)"
        }
      ],
      "gridPos": { "x": 18, "y": 6, "w": 6, "h": 6 }
    },
    {
      "title": "Database Queries",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(hikaricp_connections_acquired_total[5m]))",
          "legendFormat": "DB Queries"
        }
      ],
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 6 }
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{status=~\"5..\"}[5m]))",
          "legendFormat": "Errors"
        }
      ],
      "gridPos": { "x": 12, "y": 12, "w": 12, "h": 6 }
    }
  ],
  "schemaVersion": 17,
  "version": 1
}
